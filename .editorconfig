# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
indent_style = tab
end_of_line = crlf
trim_trailing_whitespace = true

[*.{csproj,props}]
indent_style = space
indent_size = 2

[*.cs]
indent_style = tab
indent_size = 4

[*.{xml,config}]
indent_size = 2



